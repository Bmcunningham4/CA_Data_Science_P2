    "            elif isinstance(value, str):\n",
    "                formatted_value = value  \n",
    "            else:\n",
    "                formatted_value = str(value)  \n",
    "            formatted_row.append(formatted_value)\n",
    "        formatted_table_data.append(formatted_row)\n",
